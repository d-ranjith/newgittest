#!/bin/bash

my_function() {
    # Find the latest JSON file and assign its path to a variable
    latest_file_json=$(ls -t *.json 2>/dev/null | head -n 1)
    
    if [ -n "$latest_file_json" ]; then
        echo "Latest JSON file is: $latest_file_json"
        
        # Read the contents of the JSON file and assign it to the variable
        latest_file_json_contents=$(<"$latest_file_json")
        
        # Now you can use $latest_file_json_contents as needed within this function
    else
        echo "No .json files found in the current directory."
    }
}

# Call the function
my_function
