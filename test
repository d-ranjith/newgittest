String stringValue = ConfigProvider.getConfig().getOptionalValue("myapp.someproperty", String.class).orElse("Default Value");
boolean booleanValue = ConfigProvider.getConfig().getOptionalValue("myapp.booleanProperty", Boolean.class).orElse(true);
List<String> stringListValue = ConfigProvider.getConfig().getOptionalValue("myapp.stringListProperty", List.class).orElse(Collections.emptyList());
